$(document).ready(function() {

    var map = new Datamap({element: document.getElementById('map-container')});

    $('#fullpage').fullpage({
        sectionsColor: ['rgba(223, 219, 219, 0.2)'],
        anchors: ['firstPage', 'secondPage', '3rdPage', '4thpage', 'lastPage'],
        menu: '#menu',
        scrollingSpeed: 1000
    });


});
